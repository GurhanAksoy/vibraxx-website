{
  "functions": {
    "api/cron/**/route.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "cron": [
    {
      "path": "/api/cron/round-scheduler",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/round-lifecycle",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/cron/supabase-check",
      "schedule": "0 */6 * * *"
    }
  ]
}
